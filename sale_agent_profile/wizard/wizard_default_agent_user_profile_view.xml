<odoo>

    <record model="ir.ui.view" id="wizard_default_agent_user_profile">
        <field name="name">action.default.agent.user.profile</field>
        <field name="model">wizard.default.agent.user.profile</field>
        <field name="arch" type="xml">
            <form string="Set default agent authorization user profile" >
                <footer>
                    <button string="Default"
                            name="action_set_agent_user_groups"
                            type="object"/>
                    <button string="Cancel" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>

    <act_window name="Set default agent authorization user profile"
        res_model="wizard.default.agent.user.profile"
        src_model="res.users"
        view_mode="form"
        target="new"
        key2="client_action_multi"
        id="action_default_agent_user_profile"/>

</odoo>